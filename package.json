{
	"name": "gregoryland",
	"version": "1.0.0",
	"scripts": {
		"__build": "npm run build:client && npm run build:server",
		"__build:client": "vite build --outDir dist/client --ssrManifest",
		"__build:server": "vite build --ssr src/entry.server.tsx --outDir dist/server",
		"__dev": "cross-env NODE_ENV=development node server.js",
		"__start": "cross-env NODE_ENV=production node server.js",
		"_build": "webpack",
		"_serve:prod": "PORT=80 node ./serve.js",
		"_start": "concurrently --kill-others \"PORT=8086 node ./serve.js\" \"webpack -w\"",
		"format": "prettier src/**/* --write"
	},
	"dependencies": {
		"markdown-to-jsx": "^7.1.3",
		"pu2": "^0.0.78",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.3.1",
		"react-router-dom": "^6.2.1",
		"react-transition-group": "^4.4.2",
		"react-use": "^17.2.4",
		"ua-parser-js": "^1.0.2"
	},
	"devDependencies": {
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/ua-parser-js": "^0.7.36",
		"chokidar": "^3.5.3",
		"compression": "^1.7.4",
		"concurrently": "^6.2.0",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"esbuild": "^0.14.16",
		"esbuild-plugin-manifest": "^0.5.0",
		"express": "^4.17.1",
		"kleur": "^4.1.4",
		"prettier": "^2.5.1",
		"sharp": "^0.28.3",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	},
	"author": "Gregory Dalton",
	"bugs": {
		"url": "https://github.com/gregolai/gregoryland.com/issues"
	},
	"description": "https://gregoryland.com",
	"engines": {
		"node": ">=16.13",
		"npm": ">=8.1"
	},
	"homepage": "https://gregoryland.com",
	"keywords": [
		"@gregolai",
		"gregoryland",
		"Gregory Dalton",
		"Greg Dalton"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/gregolai/gregoryland.com.git"
	}
}
